<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="16dp"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_toId_item_together_parter"
        style="@style/a_text_black_18sp_left_center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_id_item_together_parter"
        style="@style/a_text_black_18sp_left_center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_userId_item_together_parter"
        style="@style/a_text_black_18sp_left_center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_info_item_together_parter"
        style="@style/a_text_black_18sp_left_center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_romveSomeone_item_together_parter"
        android:text="移除他"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>